{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "pnpm build && pnpm deploy-application",
    "clean-deploy": "pnpm delete-and-unregister && pnpm run deploy",
    "deploy-application": "pnpm deploy-app ./dist testnet $npm_package_nearDeployConfig_filestoreContract $npm_package_nearDeployConfig_deployerAccount $npm_package_nearDeployConfig_application",
    "delete-and-unregister": "pnpm delete-app-and-unregister testnet $npm_package_nearDeployConfig_filestoreContract $npm_package_nearDeployConfig_deployerAccount $npm_package_nearDeployConfig_application 1",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chain-deployed-ui/presets": "workspace:*",
    "@near-wallet-selector/modal-ui": "^8.9.11"
  },
  "devDependencies": {
    "@near-js/accounts": "^1.2.0",
    "@near-js/keystores-node": "^0.0.11",
    "@near-js/providers": "^0.2.1",
    "@near-js/signers": "^0.1.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-gzip": "^3.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "nearDeployConfig": {
    "application": "react-vite-example",
    "deployerAccount": "gornt.testnet",
    "filestoreContract": "pfs1.testnet"
  }
}
